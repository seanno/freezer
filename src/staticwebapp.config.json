{
    "routes": [ 
        {
            "route": "/.auth/login/aad",
            "allowedRoles": [ "anonymous", "authenticated" ]
        },{
            "route": "/index.html",
            "allowedRoles": [ "contributor" ]
        },{
            "route": "/azure-storage-blob.js",
            "allowedRoles": [ "contributor" ]
        },{
            "route": "/api/*",
            "allowedRoles": [ "contributor" ]
        },{
            "route": "/",
            "statusCode": 302,
            "redirect": "/index.html"
        }
    ],
    "responseOverrides": {
        "401": { 
            "statusCode": 302,
            "redirect": "/.auth/login/aad"
        }
    }
}
